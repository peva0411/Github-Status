{
    "manifest_version": 2,

    "name": "GitHubStatusTab",
    "description": "Status of Github account in start up tab",
    "version": "1.0",

    "permissions": ["https://avatars.githubusercontent.com"],
    "background": {
        "scripts": [ "js/vendor/angular.min.js" ]
    },
    "content_security_policy": "script-src 'self';object-src 'self'",
    "chrome_url_overrides": { 
        "newtab": "tab.html"
    }
}